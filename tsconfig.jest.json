{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "jsx": "react-jsx",
      "lib": ["dom", "es6"],
      "esModuleInterop": true,
      "allowJs": true,
      "noEmit": true,
      "isolatedModules": true,
      "strict": true
    },
    "include": ["**/*.ts", "**/*.tsx", "**/*.js"],
    "exclude": ["node_modules"]
  }
  